{
  "name": "garfield-lee-resume",
  "private": true,
  "description": "Garfield Lee's resume.",
  "version": "1.0.0",
  "author": "Garfield Lee <me@550.moe>",
  "dependencies": {
    "@carbon/icons-react": "^10.8.1",
    "carbon-components": "^10.9.2",
    "carbon-components-react": "^7.9.2",
    "classnames": "^2.2.6",
    "gatsby": "^2.19.1",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-canonical-urls": "^2.1.19",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-plugin-typescript": "^2.1.26",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.13",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "sass": "^1.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/carbon-components-react": "^7.6.2",
    "@types/carbon__icons-react": "^10.6.0",
    "@types/classnames": "^2.2.9",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.14",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.18.0",
    "gatsby-plugin-sass-resources": "^2.0.0",
    "graphql": "^14.5.8",
    "husky": "^4.1.0",
    "lint-staged": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-scss": "^3.13.0",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "resume",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "check": "tsc",
    "develop": "gatsby develop",
    "format": "prettier --write '**/*.{ts,tsx,md}'",
    "lint": "npm-run-all lint:script lint:style",
    "lint:script": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:style": "stylelint 'src/**/*.scss' --fix",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Garfield550/resume"
  },
  "bugs": {
    "url": "https://github.com/Garfield550/resume/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,css,scss,html,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
