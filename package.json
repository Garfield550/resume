    "gatsby-source-filesystem": "^2.3.37",
    "gatsby-transformer-sharp": "^2.5.20",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.27.0",
    "sharp": "^0.26.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/carbon-components-react": "^7.22.0",
    "@types/carbon__icons-react": "^10.19.0",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.5",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
