    "carbon-icons": "^7.0.7",
    "classnames": "^2.2.6",
    "gatsby": "^2.24.89",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-canonical-urls": "^2.3.13",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-offline": "^3.2.38",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sass": "^2.3.22",
