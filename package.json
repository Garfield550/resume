    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "gatsby-plugin-sass-resources": "^2.0.0",
    "graphql": "^15.4.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "resume",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "check": "tsc",
    "develop": "gatsby develop",
    "format": "prettier --write '**/*.{ts,tsx,md}'",
    "lint": "npm-run-all lint:script lint:style",
    "lint:script": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:style": "stylelint 'src/**/*.scss' --fix",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Garfield550/resume"
  },
  "bugs": {
    "url": "https://github.com/Garfield550/resume/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,html,md}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
